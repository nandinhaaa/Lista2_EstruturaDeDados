#include <stdio.h>

int main() {
  long int a,b,c;
  int n,conta;
  double r;

  printf("Digite quantos termos da sequencia voce quer da Fibonacci:\n");
  scanf("%d", &n);

  if (n <= 1) {
    printf("Numero de termos invalidos!!! \n");
  }
  else {
    
    a = 1;
    b = 1;
    conta = 2;
    printf("\n\n1\n1\n");

    while(cont < n) {
      c = a + b;
      r = c;
      r = r/b;
      printf("%ld %.15lf\n", c, r);
      a = b;
      b = c;
      conta = conta + 1;
    }
  }

  return(0);
}
